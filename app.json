{
  "expo": {
    "name": "deal-mobile",
    "slug": "deal-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon-clock.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/icon-clock.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dealmobile",
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch",
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icon-clock.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dealmobile",
      "permissions": [
        "android.permission.BLUETOOTH",
        "android.permission.BLUETOOTH_ADMIN",
        "android.permission.BLUETOOTH_CONNECT"
      ]
    },
    "web": {
      "favicon": "./src/assets/icon-clock.png"
    },
    "runtimeVersion": "1.0.0",
    "plugins": [
      "@config-plugins/react-native-ble-plx",
      "react-native-ble-plx"
    ],
    "extra": {
      "eas": {
        "projectId": "bb80f571-7532-400d-954c-8ea2da8f92d1"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/bb80f571-7532-400d-954c-8ea2da8f92d1"
    }
  }
}
