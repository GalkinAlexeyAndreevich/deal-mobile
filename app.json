{
  "expo": {
    "name": "deal-mobile",
    "slug": "deal-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/deal.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/deal.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dealmobile",
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch",
          "remote-notification"
        ]
      }
