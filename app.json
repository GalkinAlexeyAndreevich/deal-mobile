    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/deal.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dealmobile"
    },
    "web": {
      "favicon": "./src/assets/deal.jpg"
    },
    "extra": {
      "eas": {
        "projectId": "0b903def-5325-4465-8f03-a857dd1fd628"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/0b903def-5325-4465-8f03-a857dd1fd628"
    },
    "plugins": [
      "@config-plugins/react-native-ble-plx",
      "react-native-ble-plx"
    ]
  }
}
