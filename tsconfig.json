{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "target": "es6",
        "allowSyntheticDefaultImports": true,
        "moduleResolution": "node",
        "noEmit": false,
        "baseUrl": ".",
        "jsx": "react-native",
        "outDir":"./dist",
        "paths": {
            "@src/*":["src/*"],
            "@components/*": ["src/components/*"],
            "@assets/*": ["src/assets/*"],
            "@Pages/*": ["src/Pages/*"],
            "@routes/*": ["src/routes/*"],
            "@store/*": ["src/store/*"],
            "@utils/*": ["src/utils/*"],
            "@interfaces": ["src/interfaces.ts"],
        }
    },
    "exclude": ["node_modules"]
}
